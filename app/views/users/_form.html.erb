<%= simple_form_for @user do |f| %>

  <%= f.input :username %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :email %>
  <%= f.input :city %>
  <%= f.input :country %>
  <%= f.input :password %>
  <%= f.input :password_confirmation %>

  <%= f.submit "Submit", class: 'btn btn-dark' %>
<% end %>
